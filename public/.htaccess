Options +FollowSymLinks

RewriteEngine On

RewriteRule ^([0-9a-zA-Z]+)$ index.php?u=$1 [L,QSA]

# #Home profile page with no show-shared and no page number 
# RewriteRule ^([0-9a-zA-Z]+)/?$ index.php?u=$1 [L,QSA]
